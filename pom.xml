	</dependency>
	<dependency>
	    <groupId>org.slf4j</groupId>
	    <artifactId>slf4j-api</artifactId>
	    <version>${slf4j-api.version}</version>
	</dependency>
  </dependencies>
  
  <build>
	<finalName>excelio-1.0</finalName>
  </build>
</project>